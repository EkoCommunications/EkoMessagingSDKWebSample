{
  "presets": [ 
    ["env", {
      "targets": {
        "browsers": ["last 2 versions" ]
      }
    }],
    "flow"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "babel-plugin-transform-builtin-extend", {
        "globals": ["Error"]
      }
    ],
    [ 
      "transform-runtime", {
        "polyfill": false,
        "regenerator": true,
      }
    ],
    [
      "babel-plugin-root-import", {
        "rootPathPrefix": "~",
        "rootPathSuffix": "src"
      }
    ]
  ]
}
